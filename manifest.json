{
    "name": "Zeal Notes",
    "description": "Make and read notes for various Websites",
    "version": "0.0.1",
    "manifest_version": 3,
    "background": {
        "service_worker": "background.js"
    },
    "permissions": [
        "storage",
        "tabs"
    ],
    "host_permissions": ["<all_urls>"],
    "action": {
        "default_title": "Zeal Notes",
        "default_popup": "html/popup.html",
        "default_icon": "/assets/logo.png"
    }
}